{
  "name": "wetube",
  "version": "1.0.0",
  "description": "youtube clone website made by javascript & node.js",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && webpack --mode=development -w",
    "build:assets": "cd src && webpack --mode=production",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -r src/static build && cp -r src/views build",
    "start": "node build/init.js",
    "buildAll": "npm run build:server && npm run build:assets && npm run copyAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongsoonko/wetube.git"
  },
  "author": "yongsoonko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yongsoonko/wetube/issues"
  },
  "homepage": "https://github.com/yongsoonko/wetube#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^9.7.5",
    "aws-sdk": "^2.814.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "node-sass": "^4.10.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "sass-loader": "^7.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "nodemon": "^1.18.6",
    "prettier": "^1.15.2"
  }
}
