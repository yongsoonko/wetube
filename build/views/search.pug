extends layouts/main
include mixins/videoBlock

block content
  .search_header
    h3 Searching for: #{searchingBy}
  .search_videos
    if videos.length === 0
      h5 No Videos Found
    each item in videos
      +videoBlock({
        id: item.id,
        title: item.title,
        views: item.views,
        videoFile: item.fileUrl
      })
